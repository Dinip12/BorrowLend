@model BorrowLend.Models.ExpenseType
@{
    ViewData["Tittle"] = "Update";
}
<h1>Update</h1>
<form method="post" asp-action="Update">
    <div class="row">
        <div class="col-12">
            <div class="form-group row">
                <label class="col-1 m-1 form-control" asp-for="ExpenseTypeName"></label>
                
            </div>
            <div class="form-group row">
                <input class="col-3 form-control m-1 " asp-for="ExpenseTypeName" />
                
            </div>
            <div class="form-group row">
                <div class="col-3 m-1 form-control"></div>
                <button type="submit" class="col-3 btn btn-success m-1">Save</button>
                <button asp-action="Index" class="col-3 btn btn-danger m-1">Back</button>
            </div>
        </div>
    </div>
</form>
